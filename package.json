  "scripts": {
    "env": "env",
    "test": "NODE_ENV=test mocha require test/setup",
    "build": "npm run build:schema && npm run build:dist && npm run build:client",
    "build:client": "rm -rf dist/static && webpack --progress --colors --config webpack.prod.config.js",
    "build:dist": "babel src -d dist && cp -r config dist/ && cp package.json dist/",
    "build:schema": "babel-node ./setup/build-schema.js",
    "deploy": "ansible-playbook -i setup/inventory --ask-vault-pass setup/deploy.yml | sed 's/\\\\n/\\\n/g'",
    "dev": "babel-node ./node_modules/.bin/webpack-dev-server --progress --port 3001 --inline --hot",
    "dist": "rm -rf node_modules && rm -rf dist && mkdir dist && yarn install --production && cp -r node_modules dist/ && yarn install && npm run build",
    "lint": "eslint --ignore-pattern \"src/server/lib/middleware.js\" --ignore-pattern \"src/server/lib/util.js\" --ignore-pattern \"src/server/routes/*\" --ignore-pattern \"src/client/**/*.js\" src/client \"src/**/*.js\" || :",
    "nodemon": "nodemon src/dev.js | bunyan -o short",
    "retire": "retire || :",
    "start": "npm run nodemon"
  },
  "metadata": {
    "graphql": {
      "schema": "./src/server/schema.json"
    }
  },
