{
  "author": "strekmann",
  "name": "nidarholmjs",
  "description": "project",
  "version": "0.1.0",
  "homepage": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/strekmann/nidarholmjs"
  },
  "scripts": {
    "env": "env",
    "build-project": "yarn build:schema && yarn relay && yarn build:server && yarn build:client",
    "build:client": "webpack --mode production --no-info --config webpack.production.config.js",
    "build:server": "babel --extensions=.tsx,.ts,.js src -d dist",
    "build:schema": "babel-node --extensions=.ts,.js ./scripts/build-schema.ts",
    "cypress": "cypress open",
    "deploy": "ansible-playbook -i ansible/inventory --ask-vault-pass ansible/deploy.yml",
    "setup": "ansible-playbook -i ansible/inventory --ask-vault-pass ansible/setup.yml",
    "dev": "babel-node ./node_modules/.bin/webpack-dev-server --mode development --progress --port 3001 --hot",
    "lint": "eslint --ignore-pattern \"src/server/lib/middleware.js\" --ignore-pattern \"src/server/lib/util.js\" --ignore-pattern \"src/server/routes/*\" \"cypress/**/*ts\" \"src/**/*.js\"",
    "nodemon": "nodemon --exec babel-node src/dev.ts",
    "relay": "relay-compiler",
    "vm": "vagrant up",
    "prettier": "prettier --write --ignore-path .gitignore \"src/**/*(js|ts|tsx)\""
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": {
    "trailingComma": "all"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      ".git",
      "client/",
      "convert/",
      "dist/",
      "node_modules",
      "public/",
      "setup/",
      "test/",
      "tmp/",
      "uploaded_files/",
      "files/*",
      "target/"
    ],
    "ext": "tsx,ts,js"
  },
  "relay": {
    "src": "./src",
    "schema": "./src/server/schema.graphql",
    "language": "typescript"
  },
