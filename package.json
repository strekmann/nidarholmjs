{
  "author": "strekmann",
  "name": "nidarholmjs",
  "description": "project",
  "version": "0.1.0",
  "homepage": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/strekmann/nidarholmjs"
  },
  "engines": {
    "node": "0.12"
  },
  "scripts": {
    "env": "env",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register | bunyan -o short",
    "build-project": "npm run build:schema && npm run relay && npm run build:server && npm run build:client && npm run build:copy",
    "build:client": "webpack --progress --colors --config webpack.prod.config.js",
    "build:copy": "cp -r config dist/ && cp package.json dist/",
    "build:server": "babel src -d dist",
    "build:schema": "babel-node ./scripts/build-schema.js",
    "deploy": "ansible-playbook -i ansible/inventory --ask-vault-pass ansible/deploy.yml || :",
    "setup": "ansible-playbook -i ansible/inventory --ask-vault-pass ansible/setup.yml || :",
    "dev": "babel-node ./node_modules/.bin/webpack-dev-server --progress --port 3001 --inline --hot",
    "lint": "eslint --ignore-pattern \"src/server/lib/middleware.js\" --ignore-pattern \"src/server/lib/util.js\" --ignore-pattern \"src/server/routes/*\" --ignore-pattern \"src/client/**/*.js\" src/client \"src/**/*.js\" || :",
    "nodemon": "nodemon src/dev.js | bunyan -o short",
    "relay": "relay-compiler --src ./src --schema ./src/server/schema.graphql",
    "retire": "retire || :",
    "start": "npm run nodemon",
    "vm": "vagrant up"
  },
  "metadata": {
    "graphql": {
      "schema": "./src/server/schema.json"
    }
  },
